{
  "name": "vue-seed",
  "version": "1.0.0",
  "description": "vue template",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.dev.js --progress",
    "mock": "cross-env NODE_ENV=development MOCK_DATA=mock webpack-dev-server --config ./build/webpack.config.dev.js --progress",
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.prod.js --progress --profile --colors",
    "build:dll": "cross-env NODE_ENV=production webpack --config ./build/webpack.dll.js --progress --profile --colors",
    "add": "node ./build/script/add",
    "comp": "node ./build/script/add --c",
    "eslint": "eslint . --ext .js,.vue --fix",
    "ftp": "ftp-cli"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "validate-commit-msg"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fafer/vue-seed.git"
  },
  "homepage": "https://github.com/fafer/vue-seed",
  "keywords": [
    "webpack",
    "vue"
  ],
  "author": "fafer",
  "contributors": [],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-proposal-function-sent": "^7.2.0",
    "@babel/plugin-proposal-json-strings": "^7.2.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-proposal-throw-expressions": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "@babel/runtime": "^7.4.2",
    "@babel/runtime-corejs3": "^7.12.5",
    "@types/node": "^14.14.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-component": "^1.1.1",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.1.0",
    "file-loader": "^6.2.0",
    "ftp-cli": "^1.0.4",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "inquirer": "^7.3.3",
    "jsdom": "^16.4.0",
    "less": "^3.12.2",
    "meow": "^8.0.0",
    "mini-css-extract-plugin": "^1.3.0",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "ora": "^5.1.0",
    "postcss-import": "^13.0.0",
    "postcss-loader": "^4.0.4",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^2.1.2",
    "regenerator-runtime": "^0.13.2",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^5.3.0",
    "yargs": "^16.1.0"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "element-ui": "^2.12.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.1.0"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "android >= 4.4",
    "ios >= 8",
    "not ie <= 11",
    "maintained node versions",
    "not dead"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
